{
    "name": "juanmiguelbesada/user-bundle",
    "description": "Provides user management for your Symfony project",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Miguel Besada",
            "email": "juanmiguelbesada@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.6",
        "symfony/form": "^4.0",
        "symfony/http-kernel": "^4.0",
        "symfony/routing": "^4.0",
        "symfony/security-bundle": "^4.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JMB\\UserBundle\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15"
    }
}
